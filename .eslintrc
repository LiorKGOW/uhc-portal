{
  "root": true,
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/*setupTests.js",
          "**/*.test.js*",
          "**/*test.js*",
          "**/*.spec.js*"
        ]
      }
    ],
    "react/forbid-prop-types": [false],
    "react/require-default-props": [false],
    "react/jsx-no-target-blank": [false]
  },
  "overrides": [{
    "files": [
      "**/*index.js*"
    ],
    "rules": {
      "import/prefer-default-export": 0
    }
  }],
  "parser": "babel-eslint",
  "globals": {
    "insights": true,
    "APP_EMBEDED": true
  }
}
