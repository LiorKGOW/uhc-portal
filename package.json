  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "APP_DEVMODE": false,
      "APP_BETA": false,
      "APP_DEV_SERVER": false,
      "APP_STAGING": false,
      "OCM_SHOW_OLD_METRICS": false
    }
  },
  "scripts": {
    "build": "webpack --progress",
    "start": "make setup && concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS --prefix-colors=cyan,reset,green 'yarn dev-server --no-progress' './backend' 'yarn insights-proxy'",
    "startmock": "concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS --prefix-colors=cyan,reset,green 'yarn dev-server-for-mock --no-progress' 'mockdata/mockserver.py' 'yarn insights-proxy'",
    "dev-server": "webpack-dev-server --progress --hot",
    "dev-server-for-mock": "OCM_SHOW_OLD_METRICS=true webpack-dev-server --progress --hot",
    "insights-proxy": "make insights-proxy-check && echo Waiting on server and backend... && wait-on http://localhost:8001/ http://localhost:8010/api && yarn stop-insights-proxy; export RUNNER=${RUNNER:-podman}; source run/insights-proxy/scripts/env.sh; run/podman-or-docker.sh run -e CUSTOM_CONF=true -e PLATFORM -v $PWD/profiles/local-frontend.js:/config/spandx.config.js --net=host --rm --name insightsproxy --security-opt label=disable -p 1337:1337 docker.io/redhatinsights/insights-proxy",
    "stop-insights-proxy": "run/podman-or-docker.sh kill insightsproxy || true",
    "lint": "eslint src --ext js,jsx",
    "test": "jest --env=jsdom",
    "selenium-chrome": "run/podman-or-docker.sh run --rm --name=browser --add-host=qa.foo.redhat.com:127.0.0.1 --net=host -p 4444:4444 -p 5900:5900 -v /dev/shm:/dev/shm selenium/standalone-chrome-debug",
    "selenium-firefox": "run/podman-or-docker.sh run --rm --name=browser --add-host qa.foo.redhat.com:127.0.0.1 --net=host -p 4444:4444 -p 5900:5900 -v /dev/shm:/dev/shm selenium/standalone-firefox-debug",
    "selenium-test-chrome": "echo Waiting on insights-proxy and selenium... && wait-on --config=run/wait-on.config https-get://qa.foo.redhat.com:1337/ http-get://localhost:4444/wd/hub && mkdir -p run/output && run/podman-or-docker.sh run --name selenium-test --rm --interactive --net=host --security-opt label=disable --mount type=bind,src=$PWD/run/verification-tests,dst=/verification-tests,ro=true --mount type=bind,src=$PWD/run/private,dst=/verification-tests/private,ro=true --mount type=bind,src=$PWD/run/cucushift,dst=/cucushift,ro=true --mount type=bind,src=$PWD/run/output,dst=/output,ro=false --user=root ocm-selenium-tests bundle exec cucumber ../cucushift/ocm/login.feature ${UNATTENDED:+--profile=_debug --profile=junit}",
    "selenium-viewer": "vinagre --file=run/selenium.vnc",
    "stop": "yarn stop-insights-proxy; run/podman-or-docker.sh kill browser; run/podman-or-docker.sh kill selenium-test; pkill -f 'concurrently.*SERVER(prevent self match){0},BACKEND,INSIGHTS'"
  },
  "insights": {
    "appname": "openshift"
  }
}
