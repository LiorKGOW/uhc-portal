    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "core-js": "^3.6.1",
    "filesize": "^3.6.1",
    "history": "^4.10.1",
    "immer": "^5.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.0",
    "react": "^16.5.2",
    "react-content-loader": "^4.3.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.3.3",
    "react-markdown": "^4.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
