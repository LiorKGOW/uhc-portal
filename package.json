    "@patternfly/react-core": "^3.134.2",
    "@patternfly/react-icons": "^3.14.39",
    "@patternfly/react-table": "^2.25.6",
    "@patternfly/react-tokens": "^2.7.25",
    "@redhat-cloud-services/frontend-components": "^0.0.43",
    "@redhat-cloud-services/frontend-components-notifications": "^1.0.0",
    "@redhat-cloud-services/frontend-components-utilities": "^0.0.15",
    "@redhat-cloud-services/rule-components": "2.0.3",
    "@sentry/browser": "5.15.5",
    "axios": "^0.19.0",
