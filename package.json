  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "wait-on": "^4.0.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "APP_DEVMODE": false,
      "APP_BETA": false,
      "APP_DEV_SERVER": false,
      "APP_STAGING": false,
      "OCM_SHOW_OLD_METRICS": false
    }
  },
  "scripts": {
    "build": "webpack --progress",
    "start": "make setup && concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS --prefix-colors=cyan,reset,green 'yarn dev-server --no-progress' './backend' 'yarn insights-proxy'",
    "startmock": "concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS --prefix-colors=cyan,reset,green 'yarn dev-server-for-mock --no-progress' 'mockdata/mockserver.py' 'yarn insights-proxy'",
    "dev-server": "webpack-dev-server --progress --hot",
    "dev-server-for-mock": "OCM_SHOW_OLD_METRICS=true webpack-dev-server --progress --hot",
    "insights-proxy": "make insights-proxy-check && run/insights-proxy.sh",
    "stop-insights-proxy": "run/podman-or-docker.sh kill insightsproxy; run/podman-or-docker.sh rm --force insightsproxy; true",
    "lint": "eslint src --ext js,jsx",
    "test": "jest --env=jsdom",
    "selenium-browser": "run/selenium-browser.sh",
    "selenium-test": "run/selenium-test.sh",
    "e2e-test": "make selenium-tests-image && yarn build --no-progress && yarn e2e-test-run",
    "e2e-test-run": "concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS,BROWSER,TEST --prefix-colors=cyan,reset,green,magenta,red --handle-input --default-input-target=TEST 'webpack-dev-server' './backend' 'run/insights-proxy.sh' 'yarn selenium-browser' 'yarn selenium-test'",
    "selenium-viewer": "wait-on tcp:5900 && vinagre --file=run/selenium.vnc",
    "stop": "yarn stop-insights-proxy; run/podman-or-docker.sh rm --force browser; run/podman-or-docker.sh rm --force selenium-test; pkill --full 'concurrently.*SERVER(prevent self match){0},BACKEND,INSIGHTS'; true"
  },
  "insights": {
    "appname": "openshift"
  }
}
