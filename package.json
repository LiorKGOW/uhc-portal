  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "globals": {
      "APP_DEVMODE": false,
      "APP_BETA": false,
      "APP_DEV_SERVER": false,
      "APP_API_ENV": "production",
      "OCM_SHOW_OLD_METRICS": false
    }
  },
  "scripts": {
    "build": "webpack $(run/progress-flag-if-tty.sh)",
    "start": "make setup && concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS --prefix-colors=cyan,reset,green 'yarn dev-server' './backend' 'yarn insights-proxy'",
    "startmock": "concurrently --kill-others --prefix=name --names=SERVER,BACKEND,INSIGHTS --prefix-colors=cyan,reset,green 'yarn dev-server-for-mock' 'mockdata/mockserver.py' 'yarn insights-proxy'",
    "dev-server": "webpack-dev-server $(run/progress-flag-if-tty.sh) --hot",
    "dev-server-for-mock": "OCM_SHOW_OLD_METRICS=true webpack-dev-server $(run/progress-flag-if-tty.sh) --hot",
    "insights-proxy": "make insights-proxy-check && run/insights-proxy.sh",
    "stop-insights-proxy": "run/podman-or-docker.sh kill insightsproxy; run/podman-or-docker.sh rm --force insightsproxy; true",
    "lint": "eslint src --ext js,jsx",
    "test": "jest --env=jsdom",
    "selenium-browser": "run/selenium-browser.sh",
    "selenium-test": "run/selenium-test.sh",
    "e2e-test": "make binaries node_modules run/verification-tests && yarn build && yarn e2e-test-run",
    "e2e-test-run": "concurrently --kill-others --success=first --prefix=name --names=SERVER,BACKEND,INSIGHTS,BROWSER,TEST --prefix-colors=cyan,reset,green,magenta,red --handle-input --default-input-target=TEST 'env WEBPACK_WATCH=false webpack-dev-server' './backend' 'run/insights-proxy.sh' 'yarn selenium-browser' 'yarn selenium-test'",
    "selenium-viewer": "wait-on tcp:5900 && vinagre --file=run/selenium.vnc",
    "stop": "yarn stop-insights-proxy; run/podman-or-docker.sh rm --force browser; run/podman-or-docker.sh rm --force selenium-test; pkill --full 'concurrently.*SERVER(prevent self match){0},BACKEND,INSIGHTS'; true"
  },
  "insights": {
    "appname": "openshift"
  }
}
