  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@patternfly|react-monaco-editor|monaco-editor|lodash-es|@openshift-assisted)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cache/Cypress/"
    ],
    "moduleNameMapper": {
      "@openshift-assisted/ui-lib/ocm": "<rootDir>/node_modules/@openshift-assisted/ui-lib/build/cjs/ocm",
      "@openshift-assisted/locales/([a-z]{2,3}/translation.json)": "<rootDir>/node_modules/@openshift-assisted/locales/lib/$1/translation.json",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^@testUtils": "<rootDir>/src/testUtils.tsx",
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "globals": {
      "APP_DEVMODE": false,
      "APP_BETA": false,
      "APP_DEV_SERVER": false,
      "APP_API_ENV": false,
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node",
      "mjs"
    ]
  },
  "scripts": {
    "build": "webpack $(run/progress-flag-if-tty.sh)",
    "start": "concurrently --kill-others --prefix=name --names=SERVER,MOCK --prefix-colors=cyan,reset --passthrough-arguments 'yarn dev-server --env noproxy api-env=staging {@}' 'mockdata/mockserver.py' --",
    "dev-server": "webpack serve $(run/progress-flag-if-tty.sh)",
    "insights-proxy": "make insights-proxy-check && run/insights-proxy.sh",
    "stop-insights-proxy": "run/podman-or-docker.sh kill insightsproxy; run/podman-or-docker.sh rm --force insightsproxy; true",
    "lint": "eslint src --ext js,jsx,ts,tsx && yarn  prettier",
    "lint-staged": "lint-staged",
    "prettier": "prettier --check 'src/**/*.{js,ts,mjs,jsx,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts,mjs,jsx,tsx}'",
    "test": "TZ=America/New_York LC_ALL=en_US.utf8 node --no-compilation-cache --expose-gc ./node_modules/.bin/jest --env=jsdom --logHeapUsage",
    "test-local": "yarn test --maxWorkers=50%",
    "test-no-cache": "yarn test --no-cache",
    "selenium-with-vnc": "concurrently --kill-others --success=first --names=BROWSER,TEST --prefix-colors=magenta,blueBright --handle-input --default-input-target=TEST --passthrough-arguments 'yarn selenium-browser' 'yarn selenium-test {@}' --",
    "selenium-with-chromedriver": "concurrently --kill-others --success=first --names=BROWSER,TEST --prefix-colors=magenta,blueBright --handle-input --default-input-target=TEST --passthrough-arguments 'chromedriver --port=4444 --url-base=/wd/hub --verbose' 'env BROWSER=chrome yarn selenium-test {@}' --",
    "selenium-browser": "run/selenium-browser.sh",
    "selenium-test": "run/selenium-test.sh",
    "e2e-test": "rm -rf ./coverage/* && yarn cypress-headless --env grepTags=ci && yarn code-coverage",
    "selenium-viewer": "wait-on tcp:5900 && vinagre --file=run/selenium.vnc",
    "stop": "yarn stop-insights-proxy; run/podman-or-docker.sh rm --force browser; pkill --full 'concurrently.*SERVER(prevent self match){0},MOCK'; pkill --full 'concurrently.*BROWSER(prevent self match){0},TEST'; true",
    "prepare": "husky install",
    "gen-types": "node scripts/gen-openapi-typescript.js && prettier --write 'src/types/**/*.ts'",
    "cypress-ui": "cypress open --config baseUrl='https://prod.foo.redhat.com:1337/openshift/'",
    "cypress-headless": "cypress run --browser chrome --config baseUrl='https://prod.foo.redhat.com:1337/openshift/'",
    "code-coverage": "nyc report --reporter=text-summary"
  },
  "insights": {
    "appname": "openshift"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ]
  }
}